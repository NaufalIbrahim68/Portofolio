{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@config": [
        "config/index"
      ],
      "@types": [
        "types"
      ],
      "@icons/*": [
        "icons/*"
      ],
      "@styles/*": [
        "styles/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}